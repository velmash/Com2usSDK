def shared_pods
  pod 'GoogleSignIn'
  pod 'SnapKit'
end

target 'Com2uSTestSDK' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Com2uSTestSDK
  shared_pods

end

target 'ExampleSDK' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ExampleSDK
  shared_pods

end
